<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
controllerName="zui5_mm_ct.controller.SerialNumbers">
	<Dialog title="{i18n>simcard}">
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
			adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
			singleContainerFullSize="false">
			<Label text="{i18n>snsimcard}"/>
			<Input value="{Posting>/SerialNumberSimcard}" submit="onPressSubmitAprovedSimcard" maxLength="20"/>
			<Button type="Emphasized" text="{i18n>scanear}" press="onPressSubmitAprovedSimcard" width="50%"/>
		</f:SimpleForm>
	</Dialog>
</core:FragmentDefinition>